<%= form_with model: [prototype, comment], local: true, id: "comment_form" do |f| %>
  <div class="field">
    <%= f.label :content, "コメント" %><br />
    <%= f.text_field :content, id: "comment_content" %>
  </div>
  <div class="actions">
    <%= f.submit "送信する", class: :form__btn %>
  </div>
<% end %>

